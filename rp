Sub LRrahoituspaatos()
    Dim peopleInvolved As String
    Dim ratkaisuehdotus As String
    Dim clipboardData As MSForms.DataObject
    
    Set clipboardData = New MSForms.DataObject
    
    ' Retrieve people involved data from clipboard
    clipboardData.GetFromClipboard
    peopleInvolved = clipboardData.GetText
    
    ' Check if data was provided
    If peopleInvolved <> "" Then
        ' Paste the data into the "Hakijat" sheet
        Sheets("Hakijat").Range("A1").Value = peopleInvolved
        MsgBox "People involved data has been imported successfully."
    Else
        MsgBox "No data was provided."
    End If
    
    ' Retrieve ratkaisuehdotus data from clipboard
    clipboardData.GetFromClipboard
    ratkaisuehdotus = clipboardData.GetText
    
    ' Check if data was provided
    If ratkaisuehdotus <> "" Then
        ' Paste the data into the "Ratkaisuehdotus" sheet
        Sheets("Ratkaisuehdotus").Range("A1").Value = ratkaisuehdotus
        MsgBox "Ratkaisuehdotus data has been imported successfully."
    Else
        MsgBox "No data was provided."
    End If
End Sub
