// Get the active document
var doc = this.ActiveDoc;

// Define the output file name
var fileName = "Rahoituspäätös.pdf";

// Define the output file path
var outputPath = "Z:\\1. ASIAKIRJALAADINTA\\10350114\\" + fileName;

// Export the active document's pages to a new document
var newDoc = this.ExportPages(0, doc.GetNumPages() - 1, outputPath, 0);
