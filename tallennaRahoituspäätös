// Get the active document
var doc = this.document;

// Get the Excel application object
var excelApp = new ActiveXObject("Excel.Application");

// Get the active Excel workbook
var workbook = excelApp.ActiveWorkbook;

// Get the value of cell E28
var cellValue = workbook.ActiveSheet.Range("E28").Value;

// Combine the cell value with the file name
var outputPath = cellValue + "\\output.pdf";

// Save the active document to the specified location
doc.SaveToFile(outputPath, false);

// Close the Excel workbook
workbook.Close(false);

// Quit the Excel application
excelApp.Quit();
