Sub LRrahoituspaatos()
    Dim rpTaulukko As Worksheet
    Dim hakijatTaulukko As Worksheet
    Dim ratkaisuehdotusTaulukko As Worksheet
    
    ' Määritä taulukkomuuttujat
    Set rpTaulukko = ThisWorkbook.Sheets("rp")
    Set hakijatTaulukko = ThisWorkbook.Sheets("Hakijat")
    Set ratkaisuehdotusTaulukko = ThisWorkbook.Sheets("Ratkaisuehdotus")
    
    ' Pyydä käyttäjää kopioimaan hakijoiden tiedot PDF-tiedostosta
    MsgBox "Kopioi hakijoiden tiedot PDF-tiedostosta ja napsauta OK."
    
    ' Tyhjennä "Hakijat" taulukko
    hakijatTaulukko.Cells.Clear
    
    ' Pyydä käyttäjää liittämään hakijoiden tiedot "Hakijat" taulukkoon
    MsgBox "Liitä hakijoiden tiedot taulukkoon ja napsauta OK."
    
    ' Pyydä käyttäjää kopioimaan ratkaisuehdotuksen PDF-tiedostosta
    MsgBox "Kopioi ratkaisuehdotus PDF-tiedostosta ja napsauta OK."
    
    ' Tyhjennä "Ratkaisuehdotus" taulukko
    ratkaisuehdotusTaulukko.Cells.Clear
    
    ' Pyydä käyttäjää liittämään ratkaisuehdotus "Ratkaisuehdotus" taulukkoon
    MsgBox "Liitä ratkaisuehdotus taulukkoon ja napsauta OK."
End Sub
