// Find the field with the ID "Radio Button 16"
var field = this.getField("Radio Button 16");

// Check if the field exists and is a radio button
if (field && field.type === "radiobutton") {
  // Check the radio button
  field.value = field.exportValues[0];

  // Print the ID of the radio button
  console.println("ID:", field.name);
}
