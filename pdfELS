function DoHighlight() {
    var nTotal = 0;
    var searchWord = "Enimm√§isluototussuhde";
    var searchString = searchWord + " ";
    
    var totalPages = this.numPages;
    for (var i = 0; i < totalPages; i++) {
        var page = this.getPageNthWord(i, 0);
        var words = page.split(/\s+/);
        
        for (var j = 0; j < words.length; j++) {
            var word = words[j];
            if (word.indexOf(searchString) === 0 && word.endsWith("%")) {
                var quadPoints = this.getPageNthWordQuads(i, j);
                this.addAnnot({
                    page: i,
                    type: "Highlight",
                    rect: quadPoints,
                    });
                nTotal++;
            }
        }
    }
    
    app.alert(nTotal + " words were highlighted");
}

DoHighlight();
