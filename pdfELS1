function DoHighlight() {
  var regex = /Luototussuhde\s+\d+(,\d+)?\s+%/g; // Updated regular expression pattern
  var nTotal = 0;

  for (var p = 0; p < this.numPages; p++) {
    var content = this.getPageContent(p);
    var contentStr = util.stringFromStream(content);
    var matches;
    
    while ((matches = regex.exec(contentStr)) !== null) {
      var matchText = matches[0];
      var startIndex = matches.index;
      var endIndex = startIndex + matchText.length - 1; // Adjust endIndex to exclude the last character (%)
      
      this.addAnnot({
        page: p,
        type: "Highlight",
        quads: this.getPageNthWordQuads(p, startIndex),
      });
      
      nTotal++;
    }
  }

  return nTotal;
}

var t = DoHighlight();
console.println(t + " occurrences were highlighted");
