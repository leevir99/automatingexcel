function DoHighlight(word) {
    var nTotal = 0;
    
    for (var p = 0; p < this.numPages; p++) {
        var cnt = this.getPageNumWords(p);
        
        for (var w = 0; w < cnt; w++) {
            var s = this.getPageNthWord(p, w);
            
            if (typeof s === 'string' && s.startsWith(word) && s.endsWith("%")) {
                this.addAnnot({
                    page: p,
                    type: "Highlight",
                    quads: this.getPageNthWordQuads(p, w),
                });
                nTotal++;
            }
        }
    }
    
    return nTotal;
}

var t = DoHighlight("EnimmÃ¤isluototussuhde");
console.println(t + " words were highlighted");
