function DoHighlight() {
    var nTotal = 0;

    for (var p = 0; p < this.numPages; p++) {
        var content = this.getPageContent(p);
        var matches = content.match(/Luototussuhde \d+,\d+ %/g);
        if (matches) {
            for (var i = 0; i < matches.length; i++) {
                var match = matches[i];
                var startIndex = content.indexOf(match);
                var endIndex = startIndex + match.length;

                this.addAnnot({
                    page: p,
                    type: "Highlight",
                    quads: this.getPageQuads(p, startIndex, endIndex),
                });
                nTotal++;
            }
        }
    }

    return nTotal;
}

var t = DoHighlight();
console.println(t + " words were highlighted");
